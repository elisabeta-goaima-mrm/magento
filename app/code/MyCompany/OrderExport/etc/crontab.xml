<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="mycompany_order_export_job" instance="MyCompany\OrderExport\Service\SftpExportService" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="mycompany_order_import_status_job" instance="MyCompany\OrderExport\Service\SftpImportStatusService" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
